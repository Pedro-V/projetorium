@startuml
!includesub base.puml!Projeto

class TelaConsulta <<boundary>> {
  informarTermoConsulta()
  selecionarFiltros()
  exibirFiltros()
  preencherFiltros()
  confirmarConsulta()
}
class TelaResultados <<boundary>> {
  exibeResultados()
}
class CtrConsultarProjeto <<control>> {
  processarConsulta()
  verificarVisibilidade()
  verificarCriterios()
}

class Filtrador <<control>> {
  cumpreFiltro()
  insereProjeto()
  descartaProjeto()
}

class GeradorResultados <<control>> {
  gerarResultados()
  informarResultados()
}

CtrConsultarProjeto -- Filtrador
Filtrador -- GeradorResultados
CtrConsultarProjeto -- TelaConsulta
CtrConsultarProjeto -- Projeto
GeradorResultados -- TelaResultados
@enduml
